using namespace std;
#include<bits/stdc++.h>

bool isValidSubsequence(vector<int> array, vector<int> sequence) {
  // Write your code here.
	int j=0,i=0;
	
	while(i<array.size() && j<sequence.size()){
		
		if(sequence[j]==array[i]){
			j++;	
		}
		i++;
		
	}
  if(j==sequence.size()){
		return true;
	}
	else{
		return false;
	}
}
